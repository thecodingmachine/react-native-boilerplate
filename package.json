{
  "name": "Boilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "test": "jest --detectOpenHandles",
    "lint-check": "eslint .",
    "lint-fix": "eslint --fix .",
    "rename": "react-native-rename",
    "fix": "./make fix",
    "sonar": "./make sonar",
    "clear": "./make clear",
    "bundle": "./make bundle",
    "prettier-fix": "prettier --write **/*.js",
    "prettier-check": "prettier --list-different **/*.js",
    "ci-ios-beta": "./make ci ios beta",
    "ci-ios-release": "./make ci ios release",
    "ci-android-beta": "./make ci android beta",
    "ci-android-release": "./make ci android release",
    "postinstall": "npx jetify && ./make postinstall"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "i18n-js": "^3.3.0",
    "prop-types": "^15.7.2",
    "react": "16.8.6",
    "react-native": "0.60.5",
    "react-native-gesture-handler": "^1.3.0",
    "react-native-global-props": "^1.1.5",
    "react-native-localize": "^1.2.1",
    "react-native-reanimated": "^1.2.0",
    "react-native-router-flux": "4.1.0-beta.8",
    "react-native-sensitive-info": "^5.5.1",
    "react-native-size-matters": "^0.2.1",
    "react-native-unimodules": "^0.7.0-rc.1",
    "react-native-version-number": "^0.3.6",
    "react-navigation": "2.17.0",
    "react-redux": "^6.0.1",
    "redux": "^4.0.1",
    "redux-persist": "^5.10.0",
    "redux-persist-sensitive-storage": "^1.0.0",
    "redux-saga": "^1.0.2",
    "redux-thunk": "^2.3.0",
    "reduxsauce": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/runtime": "^7.5.5",
    "@react-native-community/eslint-config": "^0.0.5",
    "babel-jest": "^24.5.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.12.4",
    "eslint-plugin-react-native": "^3.6.0",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.5.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "prettier": "^1.16.4",
    "react-native-dotenv": "^0.2.0",
    "react-native-rename": "^2.4.1",
    "react-test-renderer": "16.8.6",
    "remote-redux-devtools": "^0.5.16",
    "sonarqube-scanner": "^2.5.0"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./node_modules/react-native-gesture-handler/jestSetup.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!react-native)/.+"
    ]
  },
  "engines": {
    "yarn": ">=1.17",
    "node": ">=8",
    "npm": ">=6.10"
  }
}
